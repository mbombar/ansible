{% set zone_name = item[0] %}
{% set serial = 2021052318 %}

{% block SOA %}
$ORIGIN _acme-challenge.{{ zone_name }}.
$TTL 10
@	    IN SOA    ns0.{{ zone_name }}.  bombar.crans.org. (
		      {{ serial }}	; serial
		      86400	; refresh
		      3600	; retry
		      604800	; expire
                      300	; TTL
)
{% endblock %}

{% block NS %}
@                           IN      NS	ns0.{{ zone_name }}.
{% endblock %}
